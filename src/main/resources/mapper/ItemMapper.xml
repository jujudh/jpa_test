<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com._depth.jpa.item.ItemMapper">
    <select id="selectItemList" parameterType="ItemSearchDto"
            resultType="ItemDto">
    select
        item_id,
        item_name,
        item_cmt
    from
        item
    </select>
</mapper>